		<script>
		function getList(){
				$.ajax({
					type: "POST",
					url: "/data/API_006.php",
					data: "data="+JSON.stringify({DataType:"JSON",option:3,education:NaN}),
					dataType: "JSON",
					success: function(data, status, xhr){
						if(data['code'] == 0){
							$("#Educations").text("");
							$.each(data.data, function(key, value){
							var Temp = parseInt(data.data[0].pending) + parseInt(data.data[0].apply);
							var varcants = parseInt(data.data[0].total) - parseInt(data.data[0].pendding) - parseInt(data.data[0].apply)
						if(parseInt(value.total) <= Temp){
							$("#Educations").append('<tr><td>'+value.startdatetime+'</td><td>'+value.title+'</td><td><span class="badge badge-boxed  badge-soft-danger">Full</span></td></tr>');
						}else{
							$("#Educations").append('<tr><td>'+value.startdatetime+'</td><td>'+value.title+'</td><td><span class="badge badge-boxed  badge-soft-success">Afford</span></td></tr>');
						}
					});
						setTimeout(getList,10000);
					}else{
						alert(data.data.msg);
					}
					},
				error: function(jqXHR, textStatus, errorThrown){
					alert(textStatus);
				}
			});
		}


		$(document).ready(function () {
			setTimeout(getList,0);
		});
		</script>

